parse:
  additional_commands:
    configure_package_config_file:
      flags:
      - NO_SET_AND_CHECK_MACRO
      - NO_CHECK_REQUIRED_COMPONENTS_MACRO
      kwargs:
        INSTALL_DESTINATION: '*'
    find_package_handle_standard_args:
      kwargs:
        FOUND_VAR: '*'
        REQUIRED_VARS: '*'
    generate_export_header:
      flags:
      - DEFINE_NO_DEPRECATED
      kwargs:
        EXPORT_FILE_NAME: '*'
    write_basic_package_version_file:
      kwargs:
        VERSION: '*'
        COMPATIBILITY: '*'
    projname_add_library:
      flags:
      - INTERFACE
      kwargs:
        SOURCES: '*'
        DEPENDS: '*'
    projname_add_executable:
      kwargs:
        SOURCES: '*'
        DEPENDS: '*'
    projname_add_test:
      kwargs:
        SOURCES: '*'
        DEPENDS: '*'
        EXTRA_ARGS: '*'
  vartags: []
  proptags: []
format:
  line_width: 80
  tab_size: 4  # 2
  max_subgroups_hwrap: 2
  max_pargs_hwrap: 4  # 6
  max_rows_cmdline: 2
  separate_ctrl_name_with_space: false
  separate_fn_name_with_space: false
  dangle_parens: true  # false
  dangle_align: prefix
  min_prefix_chars: 0  # 4
  max_prefix_chars: 4  # 10
  max_lines_hwrap: 2
  line_ending: unix
  command_case: canonical
  keyword_case: unchanged
  always_wrap: []
  enable_sort: true
  autosort: false
  require_valid_layout: false
  layout_passes: {}
markup:
  bullet_char: '*'
  enum_char: .
  first_comment_is_literal: false
  literal_comment_pattern: null
  fence_pattern: ^\s*([`~]{3}[`~]*)(.*)$
  ruler_pattern: ^\s*[^\w\s]{3}.*[^\w\s]{3}$
  explicit_trailing_pattern: '#<'
  hashruler_min_length: 10
  canonicalize_hashrulers: true
  enable_markup: false  # true
lint:
  disabled_codes: []
  function_pattern: '[0-9a-z_]+'
  macro_pattern: '[0-9A-Z_]+'
  global_var_pattern: '[0-9A-Z][0-9A-Z_]+'
  internal_var_pattern: _[0-9A-Z][0-9A-Z_]+
  local_var_pattern: '[0-9a-z_]+'
  private_var_pattern: _[0-9a-z_]+
  public_var_pattern: '[0-9A-Z][0-9A-Z_]+'
  keyword_pattern: '[0-9A-Z_]+'
  max_conditionals_custom_parser: 2
  min_statement_spacing: 1
  max_statement_spacing: 1
  max_returns: 6
  max_branches: 12
  max_arguments: 5
  max_localvars: 15
  max_statements: 50
encode:
  emit_byteorder_mark: false
  input_encoding: utf-8
  output_encoding: utf-8
misc:
  per_command: {}
